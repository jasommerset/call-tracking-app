<% if  @calls.any? %>

  <table class="table table-striped">
    <thead>
      <tr>
        <th><%= model_class.human_attribute_name(:CallDuration) %></th>
        <th><%= model_class.human_attribute_name(:From) %></th>
        <th><%= model_class.human_attribute_name(:To) %></th>
        <th><%= model_class.human_attribute_name(:CallSid) %></th>
        <th><%= model_class.human_attribute_name(:number_id) %></th>
        <th><%= model_class.human_attribute_name(:created_at) %></th>
      </tr>
    </thead>
    <tbody>
      <% @calls.each do |call| %>
        <tr>
          <td><%= call.CallDuration %></td>
          <td><%= call.From %></td>
          <td><%= call.To %></td>
          <td><%= call.CallSid %></td>
          <td><%= call.number_id %></td>
          <td><%=l call.created_at %></td>
        </tr>
      <% end %>
    </tbody>
  </table>


<% else %>

<div class="well">Sorry, no calls yet.</div>

<% end %>




<%= link_to 'Back', numbers_path,
            :class => 'btn btn-primary pull-right' %>
