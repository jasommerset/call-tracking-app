<%- model_class = Call -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:CallDuration) %>:</strong></dt>
  <dd><%= @call.CallDuration %></dd>
  <dt><strong><%= model_class.human_attribute_name(:From) %>:</strong></dt>
  <dd><%= @call.From %></dd>
  <dt><strong><%= model_class.human_attribute_name(:To) %>:</strong></dt>
  <dd><%= @call.To %></dd>
  <dt><strong><%= model_class.human_attribute_name(:CallSid) %>:</strong></dt>
  <dd><%= @call.CallSid %></dd>
  <dt><strong><%= model_class.human_attribute_name(:number_id) %>:</strong></dt>
  <dd><%= @call.number_id %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              calls_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_call_path(@call), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              call_path(@call),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
